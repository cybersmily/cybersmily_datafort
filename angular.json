{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "homepage": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "aot": true,
            "outputPath": {
              "base": "dist"
            },
            "index": "src/index.html",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": ["zone.js"],
            "styles": [
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/css/styles.css",
              "src/css/modal.css",
              "src/css/bootstrap-overrides.css",
              "src/css/print-styles.css"
            ],
            "scripts": [],
            "browser": "src/home.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "assets": [
                "src/assets",
                "src/favicon.ico",
                "src/json",
                "src/img",
                "src/html",
                "src/.htaccess",
                "src/BingSiteAuth.xml",
                "src/sitemap.xml",
                "src/web.config"
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "assets": [
                "src/assets",
                "src/favicon.ico",
                "src/json",
                "src/img",
                "src/html",
                "src/.htaccess",
                "src/BingSiteAuth.xml",
                "src/sitemap.xml"
              ],
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false
            },
            "debug": {
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/json",
                  "output": "/json"
                },
                {
                  "glob": "**/*",
                  "input": "src/img",
                  "output": "/img",
                  "ignore": ["**/*.xcf","**/*.scss","**/*.map","**/*.sprite"]
                }

              ],
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "homepage:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "homepage:build:production"
            },
            "development": {
              "buildTarget": "homepage:build:development"
            },
            "debug": {
              "buildTarget": "homepage:build:debug"
            }
          },
          "defaultConfiguration": "debug"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "homepage:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": ["zone.js"],
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [],
            "styles": ["src/css/styles.css"],
            "sourceMap": false,
            "codeCoverage": true,
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/json",
              "src/img",
              "src/html",
              "src/web.config"
            ]
          }
        }
      }
    },
    "homepage-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "homepage:serve"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "cs",
      "style": "css",
      "standalone": false,
      "type": "component"
    },
    "@schematics/angular:directive": {
      "prefix": "cs",
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
